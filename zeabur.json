{
  "$schema": "https://schema.zeabur.app/template.json",
  "name": "Heavy Chain",
  "description": "AI駆動のアパレル向け画像生成プラットフォーム",
  "icon": "https://raw.githubusercontent.com/your-repo/heavy-chain/main/public/favicon.svg",
  "services": [
    {
      "name": "heavy-chain",
      "template": "PREBUILT",
      "spec": {
        "source": {
          "type": "GIT"
        },
        "build": {
          "buildCommand": "npm install && npm run build",
          "outputDir": "dist"
        },
        "runtime": {
          "type": "STATIC"
        },
        "env": {
          "VITE_SUPABASE_URL": {
            "default": "https://jprhgmxszvtomrqnolxn.supabase.co"
          },
          "VITE_SUPABASE_ANON_KEY": {
            "default": ""
          },
          "VITE_STRIPE_PUBLISHABLE_KEY": {
            "default": ""
          }
        }
      }
    }
  ]
}

